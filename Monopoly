import java.util.Scanner;
public class Monopoly
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0)
        {
            int a=sc.nextInt();
            int b=sc.nextInt();
            int c=sc.nextInt();
            boolean d=false;
            if((a+b)<c || (b+c)<a || (a+c)<b) d=true;
            if(d) System.out.println("YES");
            else System.out.println("NO");
        }
    }
}
